<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
  <servlet>
    <servlet-name>UserSaveServlet</servlet-name>
    <servlet-class>servlet.UserSaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserSaveServlet</servlet-name>
    <url-pattern>/userSaveServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserGetServlet</servlet-name>
    <servlet-class>servlet.UserGetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserGetServlet</servlet-name>
    <url-pattern>/userGetServlet</url-pattern>
  </servlet-mapping>
  
</web-app>